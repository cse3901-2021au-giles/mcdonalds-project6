<div>
    <h1> Add Project</h1>
    <div class = "mb-3">
        <%= form_with model: @projcet, url: new_proj_path do |form|%>
        <%= form.label :projectName%>
        <%= form.text_field :projectname,class: "form-control", placeholder: "Project Ruby"%>
    </div>
    <div class = "mb-3">
        
         <%= form.label :projecttype%>
         <%= form.text_field :projecttype,class: "form-control", placeholder: "Grou"%>
    </div>
     <div class = "mb-3">
         <%= form.label :deadline %>
         <%= form.text_field :deadline,class: "form-control", placeholder: "2/27/2021"%>
    </div>
    <div class = "submit">
        <%= form.submit "Add Project", class: "btn btn-primary"%>
    </div>
    <%end%>
</div>
